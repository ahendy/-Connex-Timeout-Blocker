{
    "manifest_version": 2,

        "name": "Connex Prevent Timeout",
        "description": "Prevents timeout when on connex for a long time.",
        "version": "1.1",

       
    
        "permissions": [
        "https://connex.csc.uvic.ca"
        ],

       "page_action": {

        "default_icon": "",
            "default_popup": "",
            "default_title": ""

    },

        "content_scripts": [{

        "matches": ["https://connex.csc.uvic.ca*"],
            "all_frames": true,
            "js": ["arrive.min.js", "contentscript.js"],
           
    }

    ]


}